<?xml version="1.0" encoding="UTF-8"?>
<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd" xmlns="http://maven.apache.org/SETTINGS/1.1.0"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <servers>
    <server>
      <username>${JF_USERNAME}</username>
      <password>${JF_PASSWORD}</password>
      <id>test-maven</id>
    </server>
  </servers>
  <profiles>
    <profile>
      <repositories>
        <repository>
          <snapshots />
          <id>test-maven</id>
          <name>test-maven</name>
          <url>https://samaeljf01.jfrog.io/artifactory/api/maven/maventest</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots />
          <id>test-maven</id>
          <name>test-maven</name>
          <url>https://samaeljf01.jfrog.io/artifactory/api/maven/maventest</url>
        </pluginRepository>
      </pluginRepositories>
      <id>artifactory</id>
    </profile>
    <profile>
      <id>Adopt11</id>
      <activation>
          <jdk>11.0.15</jdk>
      </activation>
      <properties>
          <maven.compiler.source>11.0.15</maven.compiler.source>
          <maven.compiler.target>11.0.15</maven.compiler.target>
          <maven.compiler.compilerVersion>11.0.15</maven.compiler.compilerVersion>
      </properties>
    </profile>    
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory</activeProfile>
  </activeProfiles>
</settings>
